genrule(
    outs  = [
        "foo.txt"
    ],
    name  = "__foo.txt__",
    cmd   = " ".join([
        "$(execpath //#f:bar)",
        "$(location foo.txt)",
    ]),
    exec_tools = [
        "//#f:bar"
    ]
)

